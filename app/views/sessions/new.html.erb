<div id="sign-in" class="card card-container">
  <img id="profile-img" class="profile-img-card" src="/assets/avatar.png" />
  <p id="profile-name" class="profile-name-card"></p>
  <%= form_tag sessions_path, method: "post", class: "form-user" do %>
    <input type="text" name="email" id="inputEmail" class="form-control" placeholder="Email Address" required autofocus>
    <input type="password" name="password" id="inputPassword" class="form-control" placeholder="Password" required>
    <button class="btn btn-lg btn-primary btn-block btn-signin" type="submit">Sign in</button>
  <% end %>
  <%= link_to "Create new account", new_user_path, class: "create-account" %>
</div><!-- /card-container -->
